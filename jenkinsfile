pipeline{
    agent any
    tools {maven "maven"}
    stages {
        stage('clone git') {
          steps{
            git branch: 'main', credentialsId: '1f5c4567-1fca-4363-a64c-dfe36313a2e7', url: 'https://github.com/nagadurga8/spring-petclinic.git' 
          }
        }
          stage('Build') {
              steps {
                  sh 'mvn clean package'
                  
              }
          }
    }
}